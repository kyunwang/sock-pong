{
  "name": "sock-pong-client",
  "version": "0.0.1",
  "private": true,
  "description": "Websocket client",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0",
    "develop:https": "NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop --host=0.0.0.0 --https --key-file ../certs/localhost+3-key.pem --cert-file ../certs/localhost+3.pem",
    "develop:host": "gatsby develop -o -H $HOSTNAME -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.4",
    "canvas-sketch-util": "^1.8.0",
    "dat.gui": "^0.7.6",
    "gatsby": "^2.10.4",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-styled-components": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-spring": "^8.0.27",
    "socket.io-client": "^2.2.0",
    "stats-js": "^1.0.1",
    "stats.js": "^0.17.0",
    "styled-components": "^4.3.2",
    "three": "^0.125.0",
    "three-dat.gui": "^1.0.2"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  }
}
